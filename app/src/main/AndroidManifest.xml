<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="higbee.Final">
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/garage_80"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartDrive">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".choose_car"
            android:label="@string/select_car" />
        <activity
            android:name=".new_car"
            android:label="@string/create_car" />
        <activity
            android:name=".past_drives"
            android:label="@string/view_drives" />
        <activity
            android:name=".current_drive"
            android:label="@string/current_drive" />
        <activity
            android:name=".drive_ended"
            android:label="@string/drive_ended" />
    </application>

</manifest>